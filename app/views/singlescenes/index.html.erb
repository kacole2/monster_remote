<h1>Listing singlescenes</h1>

<table>
  <thead>
    <tr>
      <th>Slot</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @singlescenes.each do |singlescene| %>
      <tr>
        <td><%= singlescene.slot %></td>
        <td><%= link_to 'Show', singlescene %></td>
        <td><%= link_to 'Edit', edit_singlescene_path(singlescene) %></td>
        <td><%= link_to 'Destroy', singlescene, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

